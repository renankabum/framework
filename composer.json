{
  "name": "navegarte/core",
  "description": "Aplicação para desenvolvimento de pequenos e grandes projetos.",
  "type": "project",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/vagnercardosoweb/framework/issues",
    "source": "https://github.com/vagnercardosoweb/framework"
  },
  "authors": [
    {
      "name": "Vagner Cardoso",
      "email": "vagner@vcwebnetworks.com.br",
      "homepage": "https://www.vcwebnetworks.com.br/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6",
    "ext-mbstring": "*",
    "ext-openssl": "*",
    "illuminate/support": "5.4.*",
    "monolog/monolog": "~1.22",
    "paragonie/random_compat": "~1.4|~2.0",
    "phpmailer/phpmailer": "~6.0",
    "slim/slim": "~3.8",
    "symfony/var-dumper": "~3.2",
    "twig/twig": "~1.18|~2.0",
    "vlucas/phpdotenv": "~2.4"
  },
  "autoload": {
    "psr-4": {
      "Core\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
